<!DOCTYPE html>
<html>
<head>
	<title>Employee list</title>
</head>
<body>
	<h1>Product List</h1>

	<a href="/homeAdmin">Back</a> |
	<a href="/logout">logout</a>

<br><br>
	<table border="1">
		<tr>
			<td>Id</td>
			<td>AuthorName</td>
			<td>BookType</td>
			<td>Price</td>
			<td>Pages</td>
			<td>Action</td>
		</tr>
	<% for(var i=0; i< product.length; i++){ %>
		<tr>
			<td><%= product[i].id %></td>
			<td><%= product[i].authorname %></td>
			<td><%= product[i].booktype %></td>
			<td><%= product[i].price %></td>
			<td><%= product[i].pages %></td>
			<td>
				<a href="/product/edit/<%= product[i].id %>"> Edit </a> |
				<a href="/product/delete/<%= product[i].id %>"> Delete </a>
			</td>
		</tr>

	<% } %>
	</table>
</body>
</html>